    "devDependencies": {
        "@types/async": "^3.2.3",
        "@types/body-parser": "^1.19.0",
        "@types/compression": "^1.7.0",
        "@types/connect-flash": "0.0.37",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/errorhandler": "^1.5.0",
        "@types/express": "^4.17.9",
        "@types/express-session": "^1.17.3",
        "@types/jest": "^25.2.3",
        "@types/lodash": "^4.14.165",
        "@types/lusca": "^1.6.2",
        "@types/morgan": "^1.9.2",
