{
    "name": "aoe-streaming-app",
    "version": "0.1.0",
    "description": "Streaming Service for the Library of Open Educational Resources",
    "repository": {
        "type": "git",
        "url": "git@gitlab.ci.csc.fi:10022/aoe/aoe-streaming-app.git"
    },
    "author": "Mika Ropponen <mroppone@csc.fi>",
