{
  "name": "koodisto-service",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "https://kalleurhoilmari@bitbucket.org/kalleurhoilmari/koodisto-service.git",
  "author": "Kalle Lehtonen <kalle.lehtonen@digia.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn run serve",
    "build": "yarn run build-ts && yarn run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "async-redis": "^1.1.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-cron": "^2.0.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "striptags": "^3.1.1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/form-data": "^2.5.0",
    "@types/node": "^12.12.8",
    "@types/node-cron": "^2.0.2",
    "@types/redis": "^2.8.14",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.45",
    "@types/xml2js": "^0.4.5",
    "concurrently": "^5.0.0",
    "express-swagger-generator": "^1.1.15",
    "nodemon": "^1.19.4",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
