        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^5.3.0",
        "connect-pg-pool": "^4.0.0",
        "eslint": "^7.32.0",
        "jest": "^26.6.3",
        "jest-junit": "^10.0.0",
        "ts-jest": "^26.4.4",
        "ts-node": "^8.10.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^3.9.7"
