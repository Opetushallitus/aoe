        "cookie-session": "^2.0.0-rc.1",
        "cors": "^2.8.5",
        "dotenv": "^8.6.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.1",
        "express-validator": "^6.6.1",
        "lodash": "^4.17.21",
        "lusca": "^1.6.0",
