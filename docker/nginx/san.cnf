[req]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = req_ext
x509_extensions    = v3_ca
prompt             = no

[req_distinguished_name]
countryName                = FI
stateOrProvinceName        = North Savo
localityName               = Kuopio
organizationName           = OPH
commonName                 = demo.aoe.fi

[req_ext]
subjectAltName = @alt_names

[v3_ca]
subjectAltName = @alt_names

[alt_names]
DNS.1   = demo.aoe.fi
