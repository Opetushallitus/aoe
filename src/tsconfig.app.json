{
    "baseUrl": "./",
    "extends": "../tsconfig.json",
    "files": [
        "main.ts",
        "polyfills.ts"
    ],
    "include": [
        "src/**/*.d.ts"
    ],
    "outDir": "../out-tsc/app",
    "paths": {
        "@angular/*": [
            "../node_modules/@angular/*"
        ],
        "@components/*": [
            "app/components/*"
        ],
        "@models/*": [
            "app/models/*"
        ],
        "@services/*": [
            "app/services/*"
        ],
        "@admin/*": [
            "app/admin/*"
        ],
        "@constants/*": [
            "app/constants/*"
        ]
    }
}
